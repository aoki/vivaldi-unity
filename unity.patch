@@ -0,0 +1,188 @@
+/* Unity Background */
+#titlebar, .tab-header, .toolbar, #tabs-container {
+  background-color: var(--colorAccentBg);
+}
+
+.tabs-top.color-behind-tabs-on #tabs-container {
+  background-image: none
+}
+
+.title {
+  font-family: 'Hiragino Sans';
+  font-weight: 300;
+  color: var(--colorAccentFg);
+}
+
+.toolbar.toolbar-addressbar:after {
+  display: none;
+}
+
+
+
+/*.tab-header {
+  background-color: var(--colorAccentBg);
+}
+
+#tabs-container {
+  background-color: var(--colorAccentBg);
+}*/
+
+
+/* Active Title Bar */
+.tab-position .tab.active .tab-header .title {
+  opacity: 1;
+  border-bottom: solid 1px var(--colorAccentFg);
+}
+
+
+
+/* Search and Address Bar */
+.addressfield, .searchfield {
+  border: none;
+  background-color: var(--colorAccentBg);
+  color: var(--colorAccentFg);
+}
+
+.addressfield.focused, .searchfield.focused {
+  border: none;
+  box-shadow: none !important;
+  border-radius: 0;
+  border-bottom: solid 1px var(--colorAccentFg);
+}
+
+input.url {
+  background-color: var(--colorAccentBg);
+  font-family: 'Hiragino Sans' !important;
+  font-weight: 100 !important;
+}
+
+.urlSearch {
+  font-family: 'Hiragino Sans';
+  font-weight: 200;
+}
+
+.pageload.progress-done {
+  opacity: 0 !important;
+  transition: all 2s;
+}
+
+/* Extension Bar */
+button.button-toolbar.browserAction-button {
+  background-color: var(--colorAccentBg) !important;
+}
+
+/* Toolbar */
+
+button.button-toolbar.home {
+  display: none;
+}
+
+button.button-toolbar.reload path,
+button.button-toolbar.loading path,
+button.button-toolbar.back path,
+button.button-toolbar.forward path {
+  display: none !important;
+}
+
+button.button-toolbar.reload>span {
+  position: absolute;
+  width: 15px;
+  height: 15px;
+  border-radius: 50%;
+  border: solid 1px;
+  border-color: var(--colorAccentFg);
+  border-right-color: transparent;
+}
+
+button.button-toolbar.reload>span:before {
+  position: absolute;
+  top: 4px;
+  right: -2px;
+  content: "";
+  height: 5px;
+  border: 3px solid transparent;
+  border-top: 4px solid;
+  background: transparent;
+  transform-origin: left top;
+  transform: rotate(-40deg);
+  box-sizing: border-box;
+  border-top-color: var(--colorAccentFg);
+}
+
+button.button-toolbar.loading>span {
+  position: absolute;
+  width: 15px;
+  height: 15px;
+  border-radius: 50%;
+  border: solid 1px;
+  border-color: var(--colorAccentFg);
+  border-right-color: transparent;
+}
+button.button-toolbar.loading>span:before {
+  display: none;
+}
+button.button-toolbar.loading>span {
+  animation: spin 1s linear infinite;
+}
+
+button.button-toolbar.back:before {
+  position: absolute;
+  left: 12px !important;
+  content: "";
+  display: inline-block !important;
+  width: 8px !important;
+  height: 8px !important;
+  border-left: 1px solid var(--colorAccentFg) !important;
+  border-bottom: 1px solid var(--colorAccentFg) !important;
+  transform: rotate(45deg) !important;
+  /* margin-right: 0.5em !important; */
+}
+
+button.button-toolbar.forward:before {
+  position: relative;
+  left: 12px !important;
+  content: "";
+  display: inline-block !important;
+  width: 8px !important;
+  height: 8px !important;
+  border-right: 1px solid var(--colorAccentFg) !important;
+  border-top: 1px solid var(--colorAccentFg) !important;
+  transform: rotate(45deg) !important;
+  /* margin-right: 0.5em !important; */
+}
+
+
+
+/* Find Pane */
+input#fip-input-text {
+  background: var(--colorBgLightIntense);
+  border: none;
+  box-shadow: none;
+  border-bottom: solid 1px var(--colorHighlightBg);
+  color: var(--colorHighlightBg);
+  font-family: 'Hiragino Sans' !important;
+  font-weight: 100 !important;
+}
+
+.toolbar.icons.fip-controls {
+  background-color: var(--colorBgLightIntense);
+}
+
+.find-in-page {
+  font-family: 'Hiragino Sans';
+  font-weight: 100;
+}
+
+button.fip-previous,
+button.fip-next,
+button.fip-close {
+  background-color: var(--colorBgLightIntense);
+  background-image: none !important;
+  border: none;
+  box-shadow: none !important;
+}
+
+/* Panel */
+.toolbar.icons {
+  background: var(--colorBgLightIntense);
+}
